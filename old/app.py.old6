                    except Exception as scheduler_error:
                        logger.error(f"Scheduler error: {scheduler_error}")
                        return f"‚ùå Error al programar recordatorio: {scheduler_error}"
                    
                    freq_text = format_interval_text(interval_minutes)
                    return f"‚úÖ ¬°Listo! He configurado tu recordatorio para {supplement_name} {freq_text}.\n\nüíä Te recordar√© tomarlo regularmente.\n\nüîç ID: {reminder_id}"
            else:
                # Usar horarios espec√≠ficos para suplementos
                times = reminder_info.get("times", ["08:00", "20:00"])
                
                created_count = 0
                created_ids = []
                
                for time_str in times:
                    try:
                        hour, minute = map(int, time_str.split(':'))
                        
                        reminder_id = save_reminder_supabase(
                            user_phone=user_phone,
                            reminder_type="supplement",
                            message=f"üíä Es hora de tomar tu {supplement_name}",
                            cron_expression=f"{minute} {hour} * * *"
                        )
                        
                        if reminder_id:
                            scheduler.add_job(
                                func=send_reminder,
                                trigger=CronTrigger(hour=hour, minute=minute),
                                args=[user_phone, f"üíä Es hora de tomar tu {supplement_name}"],
                                id=f"supplement_{supplement_name}_{user_phone}_{reminder_id}",
                                replace_existing=True
                            )
                            created_count += 1
                            created_ids.append(reminder_id)
                            
                    except Exception as e:
                        logger.error(f"Error creating supplement reminder: {str(e)}")
